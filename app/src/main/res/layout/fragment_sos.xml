<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/sos_beijing"
    tools:context="com.wanbao.fragment.SosFragment">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/dp_159"
    android:layout_marginLeft="@dimen/dp_10"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:layout_marginRight="@dimen/dp_10"
    android:background="@mipmap/yuanjiaobj"
    >
    <ImageView
        android:layout_width="@dimen/dp_200"
        android:layout_height="@dimen/dp_23"
        android:layout_marginTop="@dimen/dp_33"
        android:src="@mipmap/sos_shiyongfuwu"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0px"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_10"
        android:paddingLeft="@dimen/dp_20"
        android:id="@+id/qdsg"
        android:background="?android:attr/selectableItemBackground"
        android:gravity="center_vertical"
        android:paddingRight="@dimen/dp_20"
        android:layout_weight="1">
        <ImageView
            android:layout_width="@dimen/dp_46"
            android:layout_height="@dimen/dp_46"
            android:src="@mipmap/sos_clqdgz"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_height="@dimen/dp_46">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:textStyle="bold"
                android:textColor="@color/new_important_text"
                android:text="车辆启动故障"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_5"
                android:textColor="@color/new_secondary_text"
                android:text="一键呼叫汽车工程师"/>
        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:src="@mipmap/next_btn" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginRight="@dimen/dp_23"
        android:layout_marginLeft="@dimen/dp_23"
        android:background="@color/background"></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0px"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_20"
        android:gravity="center_vertical"
        android:paddingRight="@dimen/dp_20"
        android:layout_weight="1">
        <ImageView
            android:layout_width="@dimen/dp_46"
            android:layout_height="@dimen/dp_46"
            android:src="@mipmap/sos_clbt"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_height="@dimen/dp_46">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:textStyle="bold"
                android:textColor="@color/new_important_text"
                android:text="车辆爆胎啦"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_5"
                android:textColor="@color/new_secondary_text"
                android:text="一键呼叫补胎"/>
        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:src="@mipmap/next_btn" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginRight="@dimen/dp_23"
        android:layout_marginLeft="@dimen/dp_23"
        android:background="@color/background"></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0px"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_20"
        android:gravity="center_vertical"
        android:paddingRight="@dimen/dp_20"
        android:layout_weight="1">
        <ImageView
            android:layout_width="@dimen/dp_46"
            android:layout_height="@dimen/dp_46"
            android:src="@mipmap/sos_my"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_height="@dimen/dp_46">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:textStyle="bold"
                android:textColor="@color/new_important_text"
                android:text="没油啦"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_5"
                android:textColor="@color/new_secondary_text"
                android:text="一键呼叫送油"/>
        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:src="@mipmap/next_btn" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginRight="@dimen/dp_23"
        android:layout_marginLeft="@dimen/dp_23"
        android:background="@color/background"></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0px"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_20"
        android:gravity="center_vertical"
        android:layout_marginBottom="@dimen/dp_26"
        android:paddingRight="@dimen/dp_20"
        android:layout_weight="1">
        <ImageView
            android:layout_width="@dimen/dp_46"
            android:layout_height="@dimen/dp_46"
            android:src="@mipmap/sos_jtsg"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_height="@dimen/dp_46">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:textStyle="bold"
                android:textColor="@color/new_important_text"
                android:text="交通事故"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_5"
                android:textColor="@color/new_secondary_text"
                android:text="一键呼叫出险报警"/>
        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:src="@mipmap/next_btn" />

    </LinearLayout>

</LinearLayout>

</FrameLayout>
